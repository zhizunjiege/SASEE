<% for( let index = 0; index < data.length; index++ ) { %>
<tr class="point" data-id="<%= data[index].id %>">
    <td class="text-primary text-left ellipsis"><%= data[index].title %></td>
    <td><%= data[index].group.substr(2) %></td>
    <td><%= data[index].teacher %></td>
    <td><%= data[index].chosen %></td>
</tr>
<% } %>